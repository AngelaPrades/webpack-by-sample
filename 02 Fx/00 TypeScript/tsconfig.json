{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": false,
    "noImplicitAny": false,
    "sourceMap": true,
    "noLib": false,
    "suppressImplicitAnyIndexErrors": true,
    "lib": [
      "dom",
      "es5",
      "scripthost",
      "es2015.iterable"
    ]
  },
  "compileOnSave": false,
  "exclude": [
    "node_modules"
  ]
}
